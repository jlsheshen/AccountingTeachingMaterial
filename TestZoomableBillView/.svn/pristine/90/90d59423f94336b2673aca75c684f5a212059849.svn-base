package com.edu.subject.common;

import android.content.Context;
import android.util.AttributeSet;
import android.widget.RelativeLayout;

/**
 * 滑动菜单的主界面
 * 
 * @author lucher
 * 
 */
public class SlidingMainLayout extends RelativeLayout {

	public SlidingMainLayout(Context context) {
		super(context);
	}

	public SlidingMainLayout(Context context, AttributeSet attrs) {
		super(context, attrs);
	}

	public SlidingMainLayout(Context context, AttributeSet attrs, int defStyle) {
		super(context, attrs, defStyle);
	}

}
